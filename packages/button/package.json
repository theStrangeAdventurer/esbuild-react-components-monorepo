            "src/index.tsx"
        ]
    },
    "dependencies": {
        "@esbuild-libs/provider": ">=0.x",
        "@esbuild-libs/tsconfig": "^0.1.0",
        "@esbuild-libs/typography": ">=0.x"
    },
    "peerDependencies": {
        "react": "^18.2.0",
        "react-dom": "^18.2.0"
    },
    "devDependencies": {
        "dts-buddy": "^0.4.4"
